<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.simonebakker.simone.addemup">

    <application
        android:allowBackup="true"
        android:icon="@drawable/new_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="com.simonebakker.simone.addemup.activities.Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.simonebakker.simone.addemup.activities.GameActivity" />
        <activity android:name="com.simonebakker.simone.addemup.activities.EndOfLevel" />
        <activity android:name="com.simonebakker.simone.addemup.activities.GameOver" />
        <activity
            android:name="com.simonebakker.simone.addemup.activities.HighScores"
            android:parentActivityName="com.simonebakker.simone.addemup.activities.Menu"/>
        <activity android:name="com.simonebakker.simone.addemup.activities.PreLevel" />
    </application>

</manifest>